---
import { Image } from 'astro:assets'

export interface Props {
  name: string
  importLoc: string
  alt: string
}

const { name, importLoc, alt } = Astro.props

const dynamicImage = await import(`../assets/dog/${importLoc}.webp`)
---

<Image class={`clienthide ${alt} hidden`} src={dynamicImage.default} height={617} width={300} alt={name} />
